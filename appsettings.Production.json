{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseProvider": "SqlServer",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost\\SQLEXPRESS;Database=cvewebapp_prod;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "FileLogging": {
    "Enabled": true,
    "ActionLogPath": "/var/log/cvewebapp/actions.log",
    "ErrorLogPath": "/var/log/cvewebapp/errors.log",
    "ApplicationLoggingEnabled": true,
    "ApplicationLogPath": "/var/log/cvewebapp/application.log",
    "MinimumLogLevel": "Warning"
  }
}